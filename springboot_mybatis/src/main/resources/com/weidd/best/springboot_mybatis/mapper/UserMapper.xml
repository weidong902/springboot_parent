<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.weidd.best.springboot_mybatis.mapper.UserDao">
    <!--查询所有-->
    <select id="findAll" resultType="com.weidd.best.springboot_mybatis.pojo.User">
        select * from user
    </select>

    <insert id="add" parameterType="com.weidd.best.springboot_mybatis.pojo.User">
        insert into user (name,birthday,salary)values (#{name},#{birthday},#{salary})
    </insert>

</mapper>